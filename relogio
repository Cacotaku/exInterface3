import os


hora = 0
minuto = 0
segundo = 0
centesimo = 0
while(hora != 24):
    while(minuto != 60):
        while(segundo != 60):
            while(centesimo != 100):
                os.system('cls') or None
                print('Hor√°rio:' , hora , ':' , minuto , ':' , segundo)
                centesimo = centesimo + 1
            segundo = segundo + 1
            centesimo = 0
        segundo = 0
        minuto = minuto + 1
    minuto = 0
    hora = hora + 1
hora = 0
